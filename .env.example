# qBittorrent — fill in your actual credentials
QB_USERNAME=your_qbittorrent_username
QB_PASSWORD=your_qbittorrent_password

# Jackett — tracker search proxy (replaces PrivateHD RSS)
# After starting containers, open http://localhost:9117, copy the API key shown on the dashboard
JACKETT_URL=http://jackett:9117
JACKETT_API_KEY=your_jackett_api_key_here

# iptorrents — paste your full RSS base URL (everything EXCEPT the q= search param)
# The code appends ;q=movie+name automatically when searching
IPTORRENTS_RSS_BASE_URL=https://iptorrents.com/t.rss?u=USERID;tp=TORRENTPASS;48;20;101;7;38;100;89;90;download;s0=10

# API Security — make this a strong random string
API_KEY=generate_a_strong_random_key_here

# Jellyfin (already set, only change if rotated)
JELLYFIN_API_KEY=d5c97c8f30f1418a9573f8806b8ea334

# TMDB — for movie metadata, cover art, IMDb links (already set)
TMDB_API_KEY=0022c77a66930474249f273d4d79457b
